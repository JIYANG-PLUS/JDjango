<?xml version='1.0' encoding='utf-8'?>
<jdjango>

    <page>
        <font-size>13</font-size>
    </page>

    <env>
        <platform>Windows</platform>
        <python3>C:\Users\PC\Desktop\djangoProject\venv\Scripts\python.exe</python3>
        <port>8868</port>
        <encoding>
            <li>utf-8</li>
            <li>gbk</li>
            <li>ascii</li>
            <li>gb18030</li>
        </encoding>
        <support name="all">
            <li>darwin</li>
            <li>windows</li>
            <li>linux</li>
        </support>
        <support name="virtualenv">
            <li>darwin</li>
            <li>windows</li>
        </support>
        <database>
            <li>sqlite</li>
            <li>postgresql</li>
            <li>mysql</li>
            <li>oracle</li>
        </database>
    </env>

    <real>
        <python3>python</python3>
    </real>

    <alias>
        <file name="settings"><li>settings.py</li></file>
        <file name="manage"><li>manage.py</li></file>
        <file name="admin"><li>admin.py</li></file>
        <file name="models"><li>models.py</li></file>
        <file name="apps"><li>apps.py</li></file>
        <file name="forms"><li>forms.py</li></file>
        <file name="tests"><li>tests.py</li></file>
        <file name="urls"><li>urls.py</li></file>
        <file name="views"><li>views.py</li></file>
    </alias>

</jdjango>