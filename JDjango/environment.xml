<?xml version='1.0' encoding='utf-8'?>
<jdjango>

    <page>
        <font-size>12</font-size>
    </page>

    <env>
        <platform>Darwin</platform>
        <python3>/Users/jiyang/Desktop/venv/bin/python3.7</python3>
        <port>8868</port>
        <encoding>
            <li>utf-8</li>
