<?xml version='1.0' encoding='utf-8'?>
<jdjango>

    <page>
        <font-size>11</font-size>
    </page>

    <env>
        <python3>#!venv/Scripts/python.exe</python3>
    </env>

    <alias>

        <file name="settings">
            <li>settings.py</li>
        </file>

        <file name="manage">
            <li>manage.py</li>
        </file>

        <file name="admin">
            <li>admin.py</li>
        </file>

        <file name="models">
            <li>models.py</li>
        </file>

        <file name="apps">
            <li>apps.py</li>
        </file>

        <file name="forms">
            <li>forms.py</li>
        </file>

        <file name="tests">
            <li>tests.py</li>
        </file>

        <file name="urls">
            <li>urls.py</li>
        </file>

        <file name="views">
            <li>views.py</li>
        </file>

    </alias>

</jdjango>